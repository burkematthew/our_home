# Ignore bundler config.
/.bundle
/.licenses/*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore coverage metrics.
coverage/*

js_coverage/*

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log
# Prefer yarn.lock file
package-lock.json

/public/assets
